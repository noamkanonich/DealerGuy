                          <!-- var b = a.substring(a.indexOf('/dp/') + 4, a.indexOf('/ref')); -->
                          <a href="<%= ebay_products[i].deal %>" class="btn btn-dark my-2 my-sm-0 deal-button"> View Deal</a>
